{
  "cli": {
    "version": ">= 12.5.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "web": {
        "output": "static"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
      "web": {
        "output": "static"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
      "web": {
        "output": "static"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
